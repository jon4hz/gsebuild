{
  "name": "@swsnr/gsebuild",
  "version": "0.0.5",
  "description": "Build tooling for Gnome shell extensions",
  "type": "module",
  "homepage": "https://github.com/swsnr/gsebuild#readme",
  "bugs": {
    "url": "https://github.com/swsnr/gsebuild/issues"
  },
  "repository": "swsnr/gsebuild",
  "keywords": [
    "gnome",
    "shell",
    "extension",
    "typescript"
  ],
  "engines": {
    "node": ">=23"
  },
  "author": "Sebastian Wiesner <sebastian@swsnr.de>",
  "license": "Apache-2.0",
  "dependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@eslint/js": "^9.14.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint__js": "^8.42.3",
    "commander": "^12.1.0",
    "eslint": "^9.14.0",
    "eslint-plugin-promise": "^7.1.0",
    "execa": "^9.5.1",
    "glob": "^11.0.0",
    "ora": "^8.1.1",
    "typescript-eslint": "^8.13.0"
  },
  "scripts": {
    "prepare": "tsc --build tsconfig.json",
    "format": "prettier .",
    "lint": "eslint .",
    "clean": "rimraf dist"
  },
  "bin": {
    "gsebuild": "dist/gsebuild.js"
  },
  "exports": {
    "./tsconfig.json": "./tsconfig/tsconfig.json",
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./eslint": {
      "default": "./dist/eslint/index.js",
      "types": "./dist/eslint/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  }
}
